<div id="<%= dom_id lecture %>">
  <p>
    <strong>Name:</strong>
    <%= lecture.name %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= lecture.description %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= lecture.category.name %>
  </p>
  <p>
    <strong>Promotions:</strong>
    <%for promotion in lecture.promotions %>
      <%= promotion.name %></br>
    <% end %>
  </p>
  
</div>
